<style>
    .itemlab {
    font-size:11px;
    font-family: Helvetica;
    color: #171717;
    font-weight:normal;
    padding-top:0px;
    margin-bottom:0px;
    }
    .mycheckbox input[type="checkbox"] 
    { 
        margin-right: 5%; 
    }

    .state::before {
        display:none;
    }
    
</style>
<script type="text/javascript">
    
        function checkenter(){
            return event.keyCode != 13;
        }

</script>

<div>
