            </div>
		</ProgressTemplate>
	</asp:UpdateProgress>
    <asp:UpdatePanel runat="server" ID="upXrayRad">
        <ContentTemplate>

<div class="row" style="margin:0px;padding-bottom: 0px;background-color:white;padding-top:10px">
     <div class="col-sm-4">
        <asp:Label Font-Bold="true" Font-Size="12px" runat="server" >
            <label style="display: <%=setENG%>;"> Working Diagnosis </label>
            <label style="display: <%=setIND%>;"> Diagnosa </label>
        </asp:Label>
    </div>
</div>

<div class="row" style="margin: 0px; padding-bottom: 0px; background-color: white; padding-top: 5px">
